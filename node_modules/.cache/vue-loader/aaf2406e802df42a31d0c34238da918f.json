\"\n      @removeTodo=\"removeTodo\"\n       />\n       <div v-if=\"todos.length==0\">\n         Задач нет\n       </div>\n  </div>\n</template>\n\n<script>\n\nimport todoList from './todoList'\nimport formAdd from './formAdd'\n  export default {\n    components: {\n      todoList,\n      formAdd\n    },\n    data() {\n      return{\n        todos: [],\n        filter:'all'\n      }\n    },\n\n    created() {\n      fetch('https://jsonplaceholder.typicode.com/todos?_limit=5')\n  .then(response => response.json())\n  .then(json => {\n    setTimeout(()=>{\n        this.todos = json\n    }, 1000)\n  })\n    },\n    watch() {\n      filterList(){\n        if(this.todos === \"all\"){\n          return this.todos\n        }\n        if(this.todos === \"complited\"){\n          return this.todos.filter\n        }\n      }\n    },\n    methods: {\n      removeTodo(index) {\n        this.todos.splice(index,1)\n      },\n      addTodo(newTodo){\n        this.todos.push(newTodo)\n      }\n    }\n  }\n</script>\n\n<style>\n</style>\n"]}]}