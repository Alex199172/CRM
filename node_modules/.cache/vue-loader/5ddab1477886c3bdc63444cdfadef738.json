v class=\"uk-grid uk-grid-small\">\n        <div class=\"fs22 uk-text-left fnt-bld\">\n          Список\n        </div>\n      </div>\n\n      <div class=\"list\">\n        <div class=\"uk-margin-top uk-grid uk-grid small uk-flex-middle\" v-for=\"(item, index) in listActive\" :key=\"index\">\n          <div>\n            <div class=\"uk-text-center list__index fnt-bld uk-flex\">\n              <span class=\"uk-margin-auto uk-margin-auto-vertical\">{{ index + 1 }}</span>\n            </div>\n          </div>\n\n          <div class=\"fs18 fnt-med uk-text-left uk-width-expand\">\n            <input\n              type=\"text\"\n              name=\"materials\"\n              class=\"uk-input cl-dark border-none\"\n              v-model=\"item.name\"\n              @focus=\"titleQuizOld = item.name\"\n              @blur=\"createQuiz(index)\"\n              @keyup.enter=\"createQuiz(index)\"\n            >\n          </div>\n          <div class=\"uk-width-auto\">\n            <div class=\"uk-grid uk-grid-small\">\n              <div>\n                <router-link\n                  :to=\"`/widget/item/${index}`\"\n                  custom\n                  v-slot=\"{ navigate }\"\n                >\n                <div class=\"uk-border-circle circle-l bg-grey uk-flex\" @click=\"navigate\">\n                  <span class=\"m-auto material-icons cl-blue cr-pointer\">edit</span>\n                </div>\n                </router-link>\n              </div>\n              <div>\n                <div class=\"uk-border-circle circle-l bg-grey uk-flex\" @click=\"delQuiz(item.indexOriginal)\">\n                  <span class=\"m-auto material-icons cl-red2 cr-pointer\">delete</span>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n\n      </div>\n    </div>\n\n\n  <div class=\"uk-box-shadow-medium container uk-background-default uk-padding-small uk-margin-top brd12\">\n    <div class=\"uk-width-expand uk-grid uk-grid-small\">\n      <div>\n        <div class=\"uk-flex uk-flex-middle cr-pointer\" @click=\"addQuiz\">\n          <span class=\"material-icons uk-margin-small-right\">add_circle</span>  Добавить опрос\n        </div>\n      </div>\n    </div>\n</div>\n\n\n  </div>\n\n</template>\n\n<script>\nimport UIkit from 'uikit'\n\n  export default {\n    data() {\n      return {\n        titleQuizOld: '',\n        list: [\n          {\n            name: 'Загородное строительство',\n            status: 1,\n          },\n\n          {\n            name: 'Строительство каменных домов',\n            status: 1,\n          },\n\n          {\n            name: 'Проекты коттеджей в Москве',\n            status: 1,\n          }\n        ]\n      }\n    },\n\n    created() {\n      this._gerenateIndexesToList()\n    },\n\n    methods: {\n      _gerenateIndexesToList() {\n        this.list = this.list.map((item, index) => {\n          item['indexOriginal'] = index\n          return item\n        })\n      },\n      addQuiz() {\n        this.list.push({\n          name: 'Введите название',\n          status: 1\n        })\n      },\n\n      createQuiz(index) {\n        if (this.list[index].name === this.titleQuizOld) return\n\n        if (this.list[index].name) {\n          UIkit.modal.confirm(`Подтвердите название: \"${this.list[index].name}\"`).then(rs => {\n            console.log(rs)\n          }).catch(rs => {\n            console.log(rs)\n          })\n\n        } else {\n          this.list[index].name = this.titleQuizOld\n        }\n      },\n\n      delQuiz(index) {\n        UIkit.modal.confirm(`Подтвердите удаление: \"${this.list[index].name}\"`).then(() => {\n          this.list[index].status = 0\n        })\n      }\n    },\n\n    computed: {\n      listActive() {\n        return this.list.filter(item => item.status === 1)\n      },\n\n      listArchive() {\n        return this.list.filter(item => !item.status)\n      }\n    }\n  }\n</script>\n\n\n<style>\n  .container {\n\n  }\n\n  .list__index {\n    width: 50px;\n    height: 50px;\n    background: #e7eaf6;\n    border-radius: 12px\n  }\n</style>\n"]}]}