mplate>\n\n<script>\n\n// import radioGallery from './radioGallery'\nimport radio from './radio'\n// import checkBox from './checkBox'\n\n  export default {\n\n    components: {\n      // radioGallery,\n\n      // checkBox\n    },\n    data() {\n      return {\n        step: '',\n        list: {},\n        numberSlide: 1,\n        keysList: ''\n      }\n    },\n\n    created() {\n      this._getQuestions()\n\n    },\n\n    methods: {\n      nkj(info) {\n\n      //  this.numberSlide = info.indexSlide\n      },\n      _getQuestions () {\n        this.$api.post('/widget/get/get_info_project', {id: 35})\n       .then(rs => {\n         console.log(rs)\n         this.list = rs.data.project_info.list\n         this.step = rs.data.project_info.keysList[0]\n         this.keysList = rs.data.project_info.keysList\n         console.log(this.list)\n       })\n     },\n\n       nextSlide() {\n         if (!this.list[this.list[this.step].next] || (this.list[this.step] && Array.isArray(this.list[this.step].checked))) {\n           return\n         }\n         this.numberSlide++\n         this.step = this.list[this.step].next\n       },\n       backSlide() {\n         if (!this.list[this.list[this.step].back]) {\n           return\n         }\n         this.numberSlide--\n         this.step = this.list[this.step].back\n       }\n     }\n    }\n\n</script>\n\n\n\n\n\n<style>\n\n.widget__cont {\n  background: rgb(0 0 0 / 54%);\n  overflow-y: auto;\n}\n\n.viber__icon {\n  width: 40px !important;\n  height: 40px !important;\n}\n\n.body__title {\n  height: 350px;\n  animation-duration: 1.2s;\n  animation-name: slidein;\n}\n\n.modal {\n  border-radius: 5px;\n  padding: 15px;\n  width: 900px;\n  max-width: 900px !important;\n  margin: auto;\n  overflow-x: hidden;\n}\n\nsvg {\n  width: 20px;\n  height: 20px;\n  fill: #525858;\n}\n\n.btn {\n  background-color: #46b9b4;\n  border: none;\n  color: #fff;\n  border-radius: 2rem;\n}\n\n.btn:hover {\n  background-color: #5dd3ce;\n  transition: 0.5s;\n}\n\n.header__icon {\n  color: #46b9b4;\n}\n\n.circle {\n  width: 40px;\n  height: 40px;\n  background-color: transparent;\n  border: 50%;\n  border: solid 1px rgba(54,54,54,0.7);\n  cursor: pointer;\n}\n\n.circle:hover {\n  width: 40px;\n  height: 40px;\n  background-color: #e6e6e6;\n  border: 50%;\n  border: solid 1px rgba(54,54,54,0.7);\n  cursor: pointer;\n  transition: 0.5s;\n}\n\n@keyframes slidein {\n  from {\n    margin-left: 100%;\n    width: 300%;\n  }\n\n  to {\n    margin-left: 0%;\n    width: 100%;\n  }\n}\n\n.input__radio {\n  position: absolute;\n  z-index: -1;\n  opacity: 0;\n}\n\n.dote__label {\n  display: none;\n  fill: rgba(0,0,0,0.54)\n}\n\n.dote__label-active {\n  display: block;\n  fill: #46b9b4;\n}\n\n.circle__label-active {\n  fill: #46b9b4;\n}\n\n.input__block:hover svg {\n  border-radius: 50%;\n  -webkit-box-shadow: 0px 5px 0px 10px rgb(70, 185, 180, 0.2);\n  -moz-box-shadow: 0px 5px 0px 10px rgb(70, 185, 180, 0.2);\n  box-shadow: 0px 1px 5px 10px rgba(70, 185, 180, 0.2);\n  transition: 0.5s;\n}\n\n.image-active {\n  border: solid 1px #46b9b4;\n}\n\n.input__block {\n  border-radius: 4px;\n  border: 1px solid #e6e6e6;\n  height: 50px;\n}\n\n.input__block-active {\n  border: 1px solid #46b9b4;\n}\n\n</style>\n"]}]}