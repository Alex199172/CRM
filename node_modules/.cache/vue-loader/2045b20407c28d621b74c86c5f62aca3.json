ition-relative uk-margin-bottom uk-padding-small uk-margin-top\">\n      <div class=\"brd12\">\n        jh\n      </div>\n      <div class=\"uk-grid uk-child-width-1\">\n        <div class=\"brd12\">\n          <div class=\"list\">\n            <div class=\"uk-margin-top\" v-for=\"(item, index) in list\" :key=\"index\" :class=\"{'op05': !item.status}\">\n              <div class=\"uk-grid uk-grid-sдmall uk-flex-middle\">\n                <div>\n                  <div class=\"uk-text-center list__index fnt-bld uk-flex\">\n                    <span class=\"uk-margin-auto uk-margin-auto-vertical\">{{ index + 1 }}</span>\n                  </div>\n                </div>\n\n                <div class=\"fs18 fnt-med uk-text-left uk-width-expand\">\n                  <input\n                    typeRadio=\"text\"\n                    name=\"materials\"\n                    class=\"uk-input cl-dark border-none\"\n                    v-model=\"item.name\"\n                  >\n                </div>\n\n                <div class=\"uk-width-auto\">\n                  <div class=\"uk-grid uk-grid-small\">\n                    <div>\n                      <div class=\"uk-border-circle circle-l bg-grey uk-flex\" @click=\"toggleShowQuestion(item)\">\n                        <span class=\"m-auto material-icons cl-green cr-pointer\" v-if=\"item.status\">\n                          visibility\n                        </span>\n                        <span class=\"m-auto material-icons cr-pointer\" v-else>\n                          visibility_off\n                        </span>\n                      </div>\n                    </div>\n\n                    <div>\n                      <div class=\"uk-border-circle circle-l bg-grey uk-flex\" @click=\"delQuestion(index)\">\n                        <span class=\"m-auto material-icons cl-red2 cr-pointer\">delete</span>\n                      </div>\n                    </div>\n                  </div>\n                </div>\n              </div>\n\n              <div class=\"uk-grid uk-grid-small uk-flex-middle\">\n                <div v-for=\"(question, questionIndex) in item.answer\" :key=\"questionIndex\">\n                    <div class=\"uk-margin-small-bottom\">\n                      <input\n                        typeRadio=\"text\"\n                        name=\"materials\"\n                        class=\"uk-input\"\n                        v-model=\"question.value\"\n                      >\n                    </div>\n                </div>\n                <div class=\"cr-pointer uk-margin-small-bottom uk-flex uk-flex-middle\" @click=\"addAnswerToQuestion(index)\">\n                  <span class=\"material-icons uk-margin-small-right\">add_circle</span> Добавить ответ\n                </div>\n              </div>\n\n              <div class=\"uk-grid uk-text-left uk-margin-top uk-width-1-1 uk-flex-middle\">\n                <div class=\"uk-flex uk-flex-middle cr-pointer\" @click=\"toggleShowMoreSettings(item)\" :class=\"{'cl-blue': item.statusMoreSettings}\">\n                  <span class=\"material-icons uk-margin-small-right\">settings</span>\n                  Настройки\n                </div>\n                <div class=\"uk-flex uk-flex-middle cr-pointer\" @click=\"addDoubleQuestion(index)\">\n                  <span class=\"material-icons uk-margin-small-right\">content_copy</span> Дублировать\n                </div>\n              </div>\n\n              <div class=\"uk-margin-top\">\n                <div v-show=\"item.statusMoreSettings\" class=\"uk-text-left\">\n                  <div class=\"label label__item uk-margin-small-bottom\">\n                    <label>\n                      <input type=\"checkbox\" name=\"materials\" class=\"uk-checkbox\" v-model=\"item.typeChecbox\">\n                      <span class=\"uk-margin-small-left\">Множественный вопрос</span>\n                    </label>\n                  </div>\n                </div>\n              </div>\n              <hr v-if=\"list.length > (index + 1)\">\n            </div>\n          </div>\n        </div>\n      </div>\n\n\n      <div class=\"uk-text-left\">\n        <div class=\"btn bg-yellow brd36 fnt-med cl-wh\" @click=\"saveWidget\">\n          <div class=\"uk-grid uk-grid-small uk-flex-middle\">\n            <div class=\"uk-flex\">\n              <span class=\"material-icons\">update</span>\n            </div>\n            <div>\n              <span>Сохранить проект</span>\n            </div>\n          </div>\n        </div>\n\n      </div>\n\n\n\n    </div>\n\n    <div class=\"container uk-background-default uk-margin-top uk-margin-bottom uk-padding-small\">\n      <div class=\"uk-grid uk-grid-small\">\n        <div>\n          <div class=\"uk-flex uk-flex-middle cr-pointer\" @click=\"addQuestion\">\n            <span class=\"material-icons uk-margin-small-right\">add_circle</span>  Добавить вопрос\n          </div>\n        </div>\n      </div>\n    </div>\n\n\n\n  </div>\n\n</template>\n\n<script>\n  export default {\n    data() {\n      return {\n        id: '',\n        list: [\n\n          {\n            name: 'Площадь вашего дома',\n            typeChecbox: false,\n            status: 1,\n            statusMoreSettings: 0,\n            answer: [\n              { value: '100-150 квм'},\n              { value: '150-200 квм'},\n              { value: 'от 250 квм'}\n            ],\n          },\n\n          {\n            name: 'Количество этажей',\n            typeChecbox: false,\n            status: 1,\n            answer: [\n              { value: '1'},\n              { value: '2'},\n              { value: '3'},\n              { value: 'С мансардным этажом'},\n              { value: 'С цокольным этажом'},\n            ],\n          },\n          {\n            name: 'У вас есть земельынй участок?',\n            typeChecbox: false,\n            status: 1,\n            answer: [\n              { value: 'Да'},\n              { value: 'Нет'},\n              { value: 'Помогите подобрать'},\n            ],\n          },\n\n          {\n            name: 'Когда планируете строиться',\n            typeChecbox: false,\n            status: 1,\n            answer: [\n              { value: 'В этом сезоне'},\n              { value: 'В течении года'},\n              { value: 'Через год и более'},\n            ],\n          },\n        ],\n\n        trash: []\n      }\n    },\n\n    created() {\n      this.id = this.$route.params.id.replace(/\\D/g, '')\n\n      if (this.id) {\n        console.log(this.list)\n      }\n    },\n\n    methods: {\n      addAnswerToQuestion(questionIndex) {\n        this.list[questionIndex].answer.push({\n          value: '',\n          statusEdit: 1,\n        })\n      },\n\n      toggleShowMoreSettings(item) {\n        if (item.statusMoreSettings) {\n          item.statusMoreSettings = 0\n        } else {\n          item.statusMoreSettings = 1\n        }\n      },\n\n      toggleShowQuestion(item) {\n        if (item.status) {\n          item.status = 0\n        } else {\n          item.status = 1\n        }\n      },\n\n      delQuestion(index) {\n        this.trash.push(this.list[index])\n        this.list.splice(index, 1)\n      },\n\n      addQuestion() {\n        this.list.push({\n          name: 'Ваш вопрос',\n          typeChecbox: false,\n          status: 1,\n          statusMoreSettings: 0,\n          answer: [],\n        })\n      },\n\n      addDoubleQuestion(index) {\n        this.list.splice(index, 0, {...this.list[index]})\n      },\n\n      saveWidget() {\n        console.log(this.list)\n      }\n    }\n  }\n</script>\n\n<style>\n  .list__index {\n    width: 50px;\n    height: 50px;\n    background: #e7eaf6;\n    border-radius: 12px\n  }\n</style>\n"]}]}